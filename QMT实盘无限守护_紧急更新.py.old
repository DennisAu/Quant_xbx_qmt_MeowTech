class StartupManager:
    """å¼€æœºå¯åŠ¨ç®¡ç†å™¨ - å¥å£®å®ç°"""

    APP_NAME = "QMTå½©è™¹å®¢æˆ·ç«¯å·¥å…·"
    KEY_PATH = r"Software\Microsoft\Windows\CurrentVersion\Run"

    @classmethod
    def _get_startup_command(cls):
        """æ„å»ºå¯åŠ¨å‘½ä»¤ - ä½¿ç”¨å¤šé‡éªŒè¯ç¡®ä¿è·¯å¾„æ­£ç¡®"""
        # è·å–å½“å‰è„šæœ¬çš„ç»å¯¹è·¯å¾„
        script_path = os.path.abspath(__file__)

        # è·å–Pythonè§£é‡Šå™¨è·¯å¾„
        python_exe = sys.executable

        # éªŒè¯è·¯å¾„å­˜åœ¨æ€§
        if not os.path.exists(script_path):
            raise FileNotFoundError(f"è„šæœ¬æ–‡ä»¶ä¸å­˜åœ¨: {script_path}")
        if not os.path.exists(python_exe):
            raise FileNotFoundError(f"Pythonè§£é‡Šå™¨ä¸å­˜åœ¨: {python_exe}")

        # æ„å»ºå¯åŠ¨å‘½ä»¤
        startup_command = f'"{python_exe}" "{script_path}"'

        return startup_command, script_path

    @classmethod
    def set_startup(cls, enable):
        """è®¾ç½®å¼€æœºå¯åŠ¨ - å¢å¼ºç‰ˆå®ç°"""
        try:
            if enable:
                # æ„å»ºå¯åŠ¨å‘½ä»¤
                startup_command, script_path = cls._get_startup_command()

                # è®¾ç½®æ³¨å†Œè¡¨é¡¹
                with winreg.OpenKey(winreg.HKEY_CURRENT_USER, cls.KEY_PATH, 0, winreg.KEY_SET_VALUE) as key:
                    winreg.SetValueEx(key, cls.APP_NAME, 0, winreg.REG_SZ, startup_command)

                # ç«‹å³éªŒè¯è®¾ç½®ç»“æœ
                if cls._verify_startup_setting(startup_command):
                    log(f"âœ“ å¼€æœºå¯åŠ¨è®¾ç½®æˆåŠŸ")
                    return True
                else:
                    log(f"âœ— å¼€æœºå¯åŠ¨éªŒè¯å¤±è´¥")
                    return False
            else:
                # å–æ¶ˆå¼€æœºå¯åŠ¨
                return cls._remove_startup()

        except Exception as e:
            log(f"[å¼€æœºå¯åŠ¨] âœ— è®¾ç½®å¤±è´¥: {e}")
            return False

    @classmethod
    def _verify_startup_setting(cls, expected_command):
        """éªŒè¯å¼€æœºå¯åŠ¨è®¾ç½®æ˜¯å¦æˆåŠŸ"""
        try:
            with winreg.OpenKey(winreg.HKEY_CURRENT_USER, cls.KEY_PATH, 0, winreg.KEY_READ) as key:
                actual_value, _ = winreg.QueryValueEx(key, cls.APP_NAME)
                return actual_value == expected_command
        except Exception as e:
            log(f"éªŒè¯å¤±è´¥: {e}")
            return False

    @classmethod
    def _remove_startup(cls):
        """ç§»é™¤å¼€æœºå¯åŠ¨é¡¹"""
        try:
            # å…ˆæ£€æŸ¥æ˜¯å¦å­˜åœ¨
            with winreg.OpenKey(winreg.HKEY_CURRENT_USER, cls.KEY_PATH, 0, winreg.KEY_READ) as key:
                try:
                    winreg.QueryValueEx(key, cls.APP_NAME)
                    # å­˜åœ¨åˆ™åˆ é™¤
                    with winreg.OpenKey(winreg.HKEY_CURRENT_USER, cls.KEY_PATH, 0, winreg.KEY_SET_VALUE) as write_key:
                        winreg.DeleteValue(write_key, cls.APP_NAME)
                    log(f"âœ“ å¼€æœºå¯åŠ¨å·²å–æ¶ˆ")
                except FileNotFoundError:
                    log(f"âœ“ å¼€æœºå¯åŠ¨é¡¹ä¸å­˜åœ¨")
            return True
        except Exception as e:
            log(f"âœ— å–æ¶ˆå¼€æœºå¯åŠ¨å¤±è´¥: {e}")
            return False

    @classmethod
    def check_startup_status(cls):
        """æ£€æŸ¥å¼€æœºå¯åŠ¨çŠ¶æ€ - å¢å¼ºç‰ˆå®ç°"""
        try:
            with winreg.OpenKey(winreg.HKEY_CURRENT_USER, cls.KEY_PATH, 0, winreg.KEY_READ) as key:
                registry_value, _ = winreg.QueryValueEx(key, cls.APP_NAME)

                # è·å–å½“å‰åº”è¯¥çš„å¯åŠ¨å‘½ä»¤
                try:
                    expected_command, script_path = cls._get_startup_command()

                    # æ£€æŸ¥æ³¨å†Œè¡¨å€¼æ˜¯å¦åŒ…å«æ­£ç¡®çš„è„šæœ¬è·¯å¾„
                    is_valid = (registry_value == expected_command or
                                (script_path in registry_value and sys.executable in registry_value))

                    return is_valid

                except Exception as e:
                    log(f"æ„å»ºæœŸæœ›å‘½ä»¤å¤±è´¥: {e}")
                    # å¦‚æœæ— æ³•æ„å»ºæœŸæœ›å‘½ä»¤ï¼Œè‡³å°‘æ£€æŸ¥æ³¨å†Œè¡¨é¡¹æ˜¯å¦å­˜åœ¨
                    return bool(registry_value)

        except FileNotFoundError:
            return False
        except Exception as e:
            log(f"çŠ¶æ€æ£€æŸ¥å¤±è´¥: {e}")
            return False

    @classmethod
    def diagnose_startup(cls):
        """è¯Šæ–­å¼€æœºå¯åŠ¨é—®é¢˜"""
        try:
            # 1. æ£€æŸ¥è„šæœ¬å’ŒPythonè·¯å¾„
            startup_command, script_path = cls._get_startup_command()
        except Exception as e:
            log(f"âœ— è·¯å¾„æ£€æŸ¥å¤±è´¥: {e}")
            return False

        # 2. æ£€æŸ¥æ³¨å†Œè¡¨è®¿é—®æƒé™
        try:
            with winreg.OpenKey(winreg.HKEY_CURRENT_USER, cls.KEY_PATH, 0, winreg.KEY_READ) as key:
                pass
        except Exception as e:
            log(f"âœ— æ³¨å†Œè¡¨è¯»å–æƒé™å¼‚å¸¸: {e}")
            return False

        # 3. æ£€æŸ¥æ³¨å†Œè¡¨å†™å…¥æƒé™
        try:
            test_key = f"{cls.APP_NAME}_TEST"
            with winreg.OpenKey(winreg.HKEY_CURRENT_USER, cls.KEY_PATH, 0, winreg.KEY_SET_VALUE) as key:
                winreg.SetValueEx(key, test_key, 0, winreg.REG_SZ, "test")
                winreg.DeleteValue(key, test_key)
        except Exception as e:
            log(f"âœ— æ³¨å†Œè¡¨å†™å…¥æƒé™å¼‚å¸¸: {e}")
            return False

        # 4. æ£€æŸ¥å½“å‰çŠ¶æ€
        current_status = cls.check_startup_status()
        log(f"å½“å‰çŠ¶æ€: {'å·²å¯ç”¨' if current_status else 'æœªå¯ç”¨'}")

        return True


