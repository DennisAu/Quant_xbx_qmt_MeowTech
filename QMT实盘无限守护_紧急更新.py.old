class DataManager:
    """æ•°æ®ç®¡ç†å™¨ - æ—©ç›˜æ•°æ®æ¸…ç†"""

    def __init__(self, base_path, folder_names):
        self.base_path = base_path
        self.folder_names = [name.strip() for name in folder_names.split(',')]

    def delete_early_market_data(self):
        """åˆ é™¤æ—©ç›˜æ•°æ®"""
        log("å¼€å§‹åˆ é™¤æ—©ç›˜æ•°æ®...")
        deleted_folders = []
        failed_folders = []

        for folder_name in self.folder_names:
            folder_path = os.path.join(self.base_path, folder_name)
            if os.path.exists(folder_path) and os.path.isdir(folder_path):
                try:
                    shutil.rmtree(folder_path)
                    deleted_folders.append(folder_name)
                    log(f"æˆåŠŸåˆ é™¤æ–‡ä»¶å¤¹: {folder_path}")
                except Exception as e:
                    failed_folders.append((folder_name, str(e)))
                    log(f"åˆ é™¤æ–‡ä»¶å¤¹ {folder_path} æ—¶å‡ºé”™: {e}")
            else:
                log(f"æ–‡ä»¶å¤¹ {folder_path} ä¸å­˜åœ¨ï¼Œè·³è¿‡åˆ é™¤")

        return deleted_folders, failed_folders


