            return result
        except Exception as e:
            log(f"QMTé‡å¯è¿‡ç¨‹ä¸­å‘ç”Ÿé”™è¯¯: {e}")
            return False

    @async_operation("rainbow_restart")
    def _restart_rainbow_client(self):
