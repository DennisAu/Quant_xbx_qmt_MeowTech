            return False

    @async_operation("data_cleanup")
    def delete_early_market_data(self):
        """åˆ é™¤æ—©ç›˜æ•°æ®ï¼ˆåªåˆ é™¤æ–‡ä»¶å¤¹å†…å®¹ï¼Œä¿ç•™æ–‡ä»¶å¤¹ç»“æ„ï¼‰"""
        log("å¼€å§‹æ¸…ç†æ—©ç›˜æ•°æ®...")
        cleaned_folders = []
        failed_folders = []

        for folder_name in self.folder_names:
            folder_path = os.path.join(self.base_path, folder_name)
            try:
                if os.path.exists(folder_path) and os.path.isdir(folder_path):
                    # æ–‡ä»¶å¤¹å­˜åœ¨ï¼Œæ¸…ç©ºå†…å®¹ä½†ä¿ç•™æ–‡ä»¶å¤¹
                    items_to_delete = []
                    failed_items = []
                    
                    # å…ˆåˆ—å‡ºæ‰€æœ‰è¦åˆ é™¤çš„é¡¹ç›®
                    try:
                        items_to_delete = os.listdir(folder_path)
                    except Exception as e:
                        log(f"æ— æ³•è¯»å–æ–‡ä»¶å¤¹å†…å®¹ {folder_path}: {e}")
                        failed_folders.append((folder_name, f"æ— æ³•è¯»å–æ–‡ä»¶å¤¹å†…å®¹: {str(e)}"))
                        continue
                    
                    # é€ä¸€åˆ é™¤æ–‡ä»¶å¤¹å†…çš„å†…å®¹
                    for item in items_to_delete:
                        item_path = os.path.join(folder_path, item)
                        try:
                            if os.path.isdir(item_path):
                                shutil.rmtree(item_path)
                                log(f"åˆ é™¤å­æ–‡ä»¶å¤¹: {item_path}")
                            else:
                                os.remove(item_path)
                                log(f"åˆ é™¤æ–‡ä»¶: {item_path}")
                        except Exception as e:
                            failed_items.append(f"{item}: {str(e)}")
                            log(f"åˆ é™¤æ–‡ä»¶/æ–‡ä»¶å¤¹ {item_path} æ—¶å‡ºé”™: {e}")
                    
                    if failed_items:
                        failed_folders.append((folder_name, f"éƒ¨åˆ†å†…å®¹åˆ é™¤å¤±è´¥: {'; '.join(failed_items)}"))
                        log(f"æ–‡ä»¶å¤¹ {folder_path} éƒ¨åˆ†å†…å®¹æ¸…ç†å¤±è´¥")
                    else:
                        cleaned_folders.append(folder_name)
                        log(f"æˆåŠŸæ¸…ç©ºæ–‡ä»¶å¤¹å†…å®¹: {folder_path}")
                        
                elif os.path.exists(folder_path) and not os.path.isdir(folder_path):
                    # è·¯å¾„å­˜åœ¨ä½†ä¸æ˜¯æ–‡ä»¶å¤¹
                    failed_folders.append((folder_name, "è·¯å¾„å­˜åœ¨ä½†ä¸æ˜¯æ–‡ä»¶å¤¹"))
                    log(f"è·¯å¾„ {folder_path} å­˜åœ¨ä½†ä¸æ˜¯æ–‡ä»¶å¤¹")
                else:
                    # æ–‡ä»¶å¤¹ä¸å­˜åœ¨ï¼Œåˆ›å»ºç©ºç™½æ–‡ä»¶å¤¹
                    os.makedirs(folder_path, exist_ok=True)
                    cleaned_folders.append(folder_name)
                    log(f"æˆåŠŸåˆ›å»ºç©ºç™½æ–‡ä»¶å¤¹: {folder_path}")
                    
            except Exception as e:
                failed_folders.append((folder_name, str(e)))
                log(f"å¤„ç†æ–‡ä»¶å¤¹ {folder_path} æ—¶å‡ºé”™: {e}")
        
        # æœ€ç»ˆæ£€æŸ¥ï¼šç¡®ä¿æ‰€æœ‰ç›®æ ‡æ–‡ä»¶å¤¹éƒ½å­˜åœ¨ï¼ˆé˜²å¾¡æ€§ç¼–ç¨‹ï¼‰
        log("æ‰§è¡Œæœ€ç»ˆæ–‡ä»¶å¤¹æ£€æŸ¥...")
        for folder_name in self.folder_names:
            folder_path = os.path.join(self.base_path, folder_name)
            if not os.path.exists(folder_path):
                try:
                    os.makedirs(folder_path, exist_ok=True)
                    log(f"æœ€ç»ˆæ£€æŸ¥ï¼šé‡æ–°åˆ›å»ºæ–‡ä»¶å¤¹ {folder_path}")
                    # å¦‚æœä¹‹å‰æ²¡æœ‰æˆåŠŸè®°å½•ï¼Œç°åœ¨è¡¥å……è®°å½•
                    if folder_name not in cleaned_folders:
                        cleaned_folders.append(folder_name)
                except Exception as e:
                    log(f"æœ€ç»ˆæ£€æŸ¥ï¼šåˆ›å»ºæ–‡ä»¶å¤¹å¤±è´¥ {folder_path}: {e}")
                    if folder_name not in [item[0] for item in failed_folders]:
                        failed_folders.append((folder_name, f"æœ€ç»ˆæ£€æŸ¥åˆ›å»ºå¤±è´¥: {str(e)}"))
            else:
                log(f"æœ€ç»ˆæ£€æŸ¥ï¼šæ–‡ä»¶å¤¹å·²å­˜åœ¨ {folder_path}")
        
        return cleaned_folders, failed_folders

    def _shutdown_qmt(self):
        """å…³é—­QMT - ä¼˜åŒ–ç‰ˆ"""
        return self._execute_process_operation(
            operation_name="å…³é—­QMT",
            process_name=Constants.QMT_PROCESS_NAME,
