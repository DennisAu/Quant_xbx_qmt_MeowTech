        )

    def _shutdown_rainbow(self):
        """å…³é—­å½©è™¹å®¢æˆ·ç«¯ - ä¼˜åŒ–ç‰ˆ"""
        exe_path = self.config.get('rainbow_exe_path')
        exe_name = os.path.basename(exe_path)

        return self._execute_process_operation(
            operation_name="å…³é—­å½©è™¹å®¢æˆ·ç«¯",
            process_name=exe_name,
            operation_type="shutdown"
        )

    def _execute_process_operation(self, operation_name, process_name, exe_path=None,
                                   pre_operation=None, operation_type="restart"):
        """é€šç”¨è¿›ç¨‹æ“ä½œæ–¹æ³•"""
        log(f"å¼€å§‹æ‰§è¡Œ{operation_name}ä»»åŠ¡...")
        self.status_callback(f"æ­£åœ¨{operation_name}...")

        try:
            if pre_operation and callable(pre_operation):
                pre_operation()

            if operation_type in ["restart", "shutdown"]:
                self.status_callback(f"æ­£åœ¨å…³é—­{process_name}è¿›ç¨‹...")
                success_count, failed_count = self.process_manager.terminate_processes_by_name(process_name)
                self._report_process_status(operation_name, success_count, failed_count, "å…³é—­")

            if operation_type in ["restart", "start"] and exe_path:
                self.status_callback(f"æ­£åœ¨å¯åŠ¨{operation_name.replace('é‡å¯', '').replace('å…³é—­', '')}...")
                success, result = self.process_manager.start_process(exe_path)

                if success:
                    self.status_callback(f"âœ“ {operation_name}æˆåŠŸï¼(PID={result})")
                    return True
                else:
                    self.status_callback(f"âœ— {operation_name}å¤±è´¥: {result}")
                    return False

            if operation_type == "shutdown":
                return True

        except Exception as e:
            log(f"{operation_name}å¤±è´¥: {e}")
            self.status_callback(f"âœ— {operation_name}å¤±è´¥: {e}")
            return False

    def _report_process_status(self, operation_name, success_count, failed_count, action="æ“ä½œ"):
        if failed_count > 0:
            self.status_callback(f"âš  {operation_name}{action}éƒ¨åˆ†å¤±è´¥ (æˆåŠŸ:{success_count}, å¤±è´¥:{failed_count})")
        elif success_count > 0:
            self.status_callback(f"âœ“ {operation_name}{action}æˆåŠŸ ({success_count} ä¸ªè¿›ç¨‹)")
        else:
            self.status_callback(f"â„¹ æœªå‘ç°è¿è¡Œä¸­çš„è¿›ç¨‹")

    def _shutdown_system(self):
