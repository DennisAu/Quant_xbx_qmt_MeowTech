    @async_operation("rainbow_restart_only")
    def _restart_rainbow_client_only(self):
        """ä»…é‡å¯å½©è™¹å®¢æˆ·ç«¯ï¼ˆä¸åˆ é™¤æ•°æ®ï¼‰"""
        try:
            exe_path = self.config.get('rainbow_exe_path')
            exe_name = os.path.basename(exe_path)

